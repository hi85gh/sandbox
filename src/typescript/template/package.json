{
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --require tsconfig-paths/register --respawn --transpile-only src/main.ts",
    "start": "node --require=module-alias/register dist/main.js",
    "build": "tsc",
    "prebuild": "node --eval \"require('fs').rmdirSync('dist', { recursive: true })\"",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "module-alias": "2.2.2"
  },
  "devDependencies": {
    "@types/node": "14.14.27",
    "ts-node-dev": "1.1.6",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.2.2"
  },
  "_moduleAliases": {
    "~": "dist"
  }
}
